{
  "name": "create-react-extension-app",
  "version": "1.1.4",
  "description": "Start building an extension right-away",
  "author": "Rishabh Sinha, Harsh Jain, Noushil Sharma",
  "bin": "./bin/cli.js",
  "license": "MIT",
  "keywords": [
    "create-react-extension-app",
    "extension app",
    "react extension app",
    "starter files for react extension",
    "extension manifest v3"
  ],
  "dependencies": {
    "@craco/craco": "^6.3.0",
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@material-ui/styles": "^4.11.5",
    "@mui/material": "^5.10.15",
    "@mui/styles": "^5.10.15",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.1.0",
    "@testing-library/user-event": "^13.2.1",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.9",
    "axios": "^0.21.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.4.3",
    "react-scripts": "^4.0.3",
    "sass": "^1.42.1",
    "web-vitals": "^2.1.0",
    "webextension-polyfill": "^0.10.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "set \"INLINE_RUNTIME_CHUNK=false\" && craco build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "env": {
      "webextensions": true
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
